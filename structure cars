#include <stdio.h>
#include <stdint.h>

struct carmodel
{
    int32_t carno;
    int16_t carmodel;
    int16_t carmaxspeed;
    float   carweight;
};

int main()
{
    struct carmodel car = {1223456, 2014, 100, 4000.45};

    printf("Car Number      : %d\n", car.carno);
    printf("Car Model Year  : %d\n", car.carmodel);
    printf("Car Max Speed   : %d km/h\n", car.carmaxspeed);
    printf("Car Weight      : %.2f kg\n", car.carweight);

    return 0;
}



	}


